# ── PostgreSQL ──────────────────────────────────────────────────────
POSTGRES_USER=prefect
POSTGRES_PASSWORD=changeme_prefect_2024
POSTGRES_DB=prefect

# ── Prefect Server ──────────────────────────────────────────────────
PREFECT_UI_PORT=4200
# CATATAN: Jangan set PREFECT_API_URL di sini.
# Dari mesin lokal: export PREFECT_API_URL=https://datahub.kaltarastats.id/api
# Worker di dalam Docker sudah dikonfigurasi langsung di docker-compose.yml.

# ── Cloudflare Tunnel ───────────────────────────────────────────────
# Domain publik yang diarahkan ke server ini
PUBLIC_DOMAIN=datahub.kaltarastats.id
# Token tunnel dari Cloudflare Zero Trust dashboard
# Zero Trust → Networks → Tunnels → [nama tunnel] → Configure → Token
CLOUDFLARE_TUNNEL_TOKEN=isi_token_tunnel_anda_di_sini

# ── Prefect Worker ──────────────────────────────────────────────────
PREFECT_WORK_POOL_NAME=docker-pool
